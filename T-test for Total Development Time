from scipy.stats import ttest_ind

# Define weights for L group
L_weights = [
    23.29, 24.29, 23.29, 27.29, 27.29, 24.25, 23.29, 24.29, 27.29, 26.25,
    24.25, 26.29, 25.25, 23.25, 24.25, 26.25, 26.29
]

F_weights = [
    23.29, 25.25, 24.25, 26.29, 26.29, 25, 27.25, 23.29, 24.25,
    23.25,25.25, 23.29, 24.25, 23.25, 26.29, 24.25, 24.29, 23.29,
    23.29, 25.25, 27.25, 25.25, 28.29, 28.29, 27.29, 26.29, 24.25
]

# Perform Welch's t-test
t_stat, p_value = ttest_ind(L_weights, F_weights, equal_var=False)

# Output result
print(f"T-statistic: {t_stat:.4f}")
print(f"P-value: {p_value:.4f}")

# Interpretation
alpha = 0.05
if p_value < alpha:
    print("Result: Statistically significant difference in weights between L and F groups.")
else:
    print("Result: No statistically significant difference in weights between L and F groups.")
